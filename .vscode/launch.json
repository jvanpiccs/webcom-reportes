{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Edge",
      "request": "launch",
      "type": "pwa-msedge",
      "url": "https://claroaup.sharepoint.com/sites/webcom/_layouts/workbench.aspx",
      "webRoot": "${workspaceRoot}",
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "webpack:///.././src/*": "${webRoot}/src/*",
        "webpack:///../../../src/*": "${webRoot}/src/*",
        "webpack:///../../../../src/*": "${webRoot}/src/*",
        "webpack:///../../../../../src/*": "${webRoot}/src/*"
      },
      "runtimeArgs": ["--remote-debugging-port=9222 --incognito"]
    }
  ]
}
